//!/usr/bin/env groovy

node{
    stage('Build'){
        	sh '/opt/maven/bin/mvn -v'
        	echo 'Realizando Build...'
        	echo 'Build finalizado.'
    }
    stage('Test'){
        echo 'Realizando Teste...'
        echo 'Teste finalizado.'
    }
    stage('Deploy'){
        echo 'Realizando Deploy...'
        echo 'Deploy finalizado.'
    }


}
   
